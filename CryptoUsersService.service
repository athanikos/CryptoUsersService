[Unit]
Description=uWSGI instance to serve CryptoUsersService
After=network.target

[Service]
User=nikos
Group=nikos
WorkingDirectory=/home/nikos/CryptoUsersService
Environment="PATH=/home/nikos/CryptoUsersServiceEnv/bin"
ExecStart=/home/nikos/.local/bin/gunicorn  --bind unix:CryptoUsersService.sock   "wsgi:app()"



[Install]
WantedBy=multi-user.target

